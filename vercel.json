{
    "version": 3,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "http://todo-list-kohl-ten.vercel.app",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "DB_CONNECTION": "mysql",
      "DB_HOST": "127.0.0.1",
      "DB_PORT": "3306",
      "DB_DATABASE": "todolist_api_db",
      "DB_USERNAME": "root",
      "DB_PASSWORD": "password"
    }
  }
   
